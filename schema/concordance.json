[
  { "@type" : "Class",
    "@id" : "Book",
    "title" : "xsd:string",
    "chapters" : { "@type" : "Set",
                   "@class" : "Chapter" }},

  { "@type" : "Class",
    "@id" : "Chapter",
    "@key" : { "@type" : "Lexical",
               "@fields" : ["number"]},
    "number" : "xsd:integer",
    "documents" : { "@type" : "Array",
                    "@class" : "Document"}},

  { "@type" : "Class",
    "@id" : "Document",
    "@key" : { "@type" : "Random" },
    "text" : "xsd:string",
    "terms" : { "@type" : "Set",
                "@class" : "TermFreq"}},

  { "@type" : "Class",
    "@id" : "Term",
    "@key" : { "@type" : "Lexical",
               "@fields" : ["term"]},
    "term" : "xsd:string",
    "documents" : { "@type" : "Set",
                    "@class" : "Document-TF-IDF" },
    "df" : "xsd:integer" },

  { "@type" : "Class",
    "@id" : "TermFreq",
    "@key" : { "@type" : "Random" },
    "@subdocument" : [],
    "term" : "Term",
    "tf" : "xsd:integer" },

  { "@type" : "Class",
    "@id" : "Document-TF-IDF",
    "@key" : { "@type" : "Random" },
    "@subdocument" : [],
    "document" : "Document",
    "tf_idf" : "xsd:decimal" }
]

